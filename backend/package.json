{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/src/index.js",
    "test": "jest --runInBand --silent",
    "test:ci": "jest --silent",
    "dev": "npm run swagger:build && ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "dev:docker": "docker compose up -d tecno-db && npm run swagger:build && ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "dev:sql": "npm run swagger:build && ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias -p tsconfig.json && mkdir -p ./dist/src/docs/build && cp ./src/docs/build/swagger.yaml ./dist/src/docs/build",
    "db:seed": "cross-env SEED=true npm run db:drop && cross-env SEED=true tsx src/utils/seed/index.ts",
    "db:drop": "tsx src/utils/seed/drop.ts",
    "swagger:build": "npx swagger-cli bundle src/docs/index.yaml --outfile src/docs/build/swagger.yaml --type yaml",
    "format": "prettier --write src",
    "lint:staged": "npx lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/axios": "^0.14.0",
    "@types/socket.io": "^3.0.2",
    "axios": "^1.7.7",
    "class-validator": "0.14.1",
    "cookie": "^0.7.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "3.1.1",
    "formidable": "^3.5.1",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.3",
    "node-jose": "^2.2.0",
    "nodemon": "^3.1.7",
    "socket.io": "^4.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.6.2",
    "uuidv4": "^6.2.13",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@faker-js/faker": "9.0.3",
    "@types/connect": "^3.4.38",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/estree": "^1.0.6",
    "@types/express": "^5.0.0",
    "@types/formidable": "3.4.5",
    "@types/http-errors": "^2.0.4",
    "@types/is-ci": "3.0.4",
    "@types/jest": "29.5.13",
    "@types/json-schema": "^7.0.15",
