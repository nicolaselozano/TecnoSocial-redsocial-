  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register  ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run swagger:build && ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "db:seed": "npm run db:drop && cross-env SEED=true tsx src/utils/seed/index.ts",
