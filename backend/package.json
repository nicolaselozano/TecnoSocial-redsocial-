    "@types/send": "^0.17.4",
    "@types/serve-static": "^1.15.7",
    "@types/strip-bom": "^3.0.0",
    "@types/strip-json-comments": "^0.0.30",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "0.2.34",
    "@typescript-eslint/eslint-plugin": "8.8.1",
